{
  "name": "backend",
  "version": "1.0.0",
  "description": "Awesome project developed with TypeORM.",
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.10",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "jest-mock-extended": "^4.0.0-beta1",
    "nodemon": "^3.1.9",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "body-parser": "^1.19.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.0",
    "jsonwebtoken": "^9.0.2",
    "mysql": "^2.14.1",
    "nodemailer": "^6.9.16",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.1.7",
    "twilio": "^5.4.2",
    "typeorm": "0.3.20"
  },
  "scripts": {
    "start": "npx tsc && node dist/index.js",
    "start:dev": "npx tsc && NODE_ENV=development node dist/index.js",
    "dev": "nodemon --watch src --ext ts --exec \"npm run build && node dist/index.js",
    "build": "tsc",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "npm run typeorm migration:generate -n InitialMigration -d ./src/data-source.ts",
    "migration:apply-curr": "npm run typeorm migration:run -d ./src/data-source.ts",
    "migration:revert": "npm run typeorm migration:revert -d ./src/data-source.ts",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  }
}
